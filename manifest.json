{
    "name": "Save State",
    "version": "1.0",
    "description": "Saves all currently open tabs for you to open later. “Download” symbol used on the icon from The Noun Project collection.",
    "icons": { 
        "16": "icon_16.png", 
        "48": "icon_48.png", 
        "128": "icon_128.png" },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "Save State"
    },
    "permissions": [
                    "tabs"
                    ]
}